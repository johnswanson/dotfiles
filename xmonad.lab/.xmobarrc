Config { bgColor = "#332936"
       , fgColor = "#aaa"
       , iconRoot = "/home/jds/.icons/"
       , pickBroadest = True
       , font = "xft:Essential PragmataPro:size=18"
       , position = TopSize C 100 25
       , commands = [
			 Run Network "enp3s0"
                      [ "-t", "[<fc=#dc322f><tx></fc> <fc=#859900><rx></fc>]"
                      , "--ddigits", "0"
                      , "--suffix", "True"
                      , "-w", "6",
                        "-a", "r"
                      ] 10
                    , Run CommandReader "~/.pymodoro/pymodoro.py" "pomodoro"
										, Run Network "wlp4s0"
                      [ "-t", "[<fc=#dc322f><tx></fc> <fc=#859900><rx></fc>]"
                      , "--ddigits", "0"
                      , "--suffix", "True"
                      , "-w", "6",
                        "-a", "r"
                      ] 10
                    , Run Uptime
                      ["-t", "[<days> <hours> <minutes>]",
                       "-S", "True"] 600
                    , Run Network "net0"
                      [ "-t", "[<fc=#dc322f><tx></fc> <fc=#859900><rx></fc>]"
                      , "--ddigits", "0"
                      , "--suffix", "True"
                      , "-w", "6",
                        "-a", "r"
                      ] 10
                    , Run Network "wlp4s0"
                    [ "-t", "[<fc=#dc322f><tx></fc> <fc=#859900><rx></fc>]"
                    , "--ddigits", "0"
                    , "--suffix", "True"
                    , "-w", "6"
                    , "-a", "r"
                    ] 10
                    , Run CommandReader "~/.pymodoro/pymodoro.py" "pomodoro"
                    , Run CoreTemp
                      ["-t", "[<core0>° <core1>° <core2>° <core3>° <core4>°]",
                       "-m", "2",
                       "-L", "40",
                       "-H", "60",
                       "--high", "#dc322f"] 50
                    , Run MultiCpu
                      ["-t","[<autovbar>]",
                       "-a", "r",
                       "-m", "2",
                       "-L","3","-H","50",
                       "--low", "#859900",
                       "--high","#dc322f"] 20
                    , Run Memory
                      ["-t","[<usedvbar>]"
                      , "-L", "50"
                      , "-H", "80"
                      , "--high", "#dc322f"] 100
                    , Run Date "<fc=#6c71c4>%a %F %H:%M</fc>" "date" 600
                    , Run Com "uname" ["-r"] "" 36000
                    , Run StdinReader
                    , Run Battery
                      ["-t", "<leftvbar> <left>% <watts> <acstatus>",
                       "-L", "20", "-H", "80",
                       "--high", "#859900",
                       "--low", "#dc322f",
                       "--",
                       "-p", "#859900",
                       "-O", "<icon=ac_01.x2.xbm/>!",
                       "-i", "<icon=ac_01.x2.xbm/>",
                       "-o", "<icon=bat_full_01.x2.xbm/>"] 10
                    ]
       , alignSep = "}{"
       , template = "%StdinReader% %multicpu% %coretemp% %memory% <icon=wifi_01.x2.xbm/>%wlp4s0% <icon=net_wired.x2.xbm/>%net0% %uptime% }{ [%pomodoro%] [%battery%] [%date%] %uname%"
       }
